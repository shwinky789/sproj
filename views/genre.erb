<body id="homepage">
<div id="feed">

<link rel="stylesheet" type="text/css" href="styles/styles.css"/>
<link rel="stylesheet" type="text/css" href="../styles/styles.css"/>

<%@posts.reverse.each do |t|%>
	<a href="/prof/<%=User.find(t.user_id).username%>"><h2> <%= User.find(t.user_id).username%> </h2></a>
	<h3> <%= t.content %> </h3>
		<iframe width="560" height="315" src="//www.youtube.com/embed/<%=t.song%>" frameborder="0" allowfullscreen></iframe>
	<h6> <%= t.created_at%> </h6>
	<h6> <%= t.genre%> </h6>
	<hr>
<%end%>
</div>
</body>

<div id="homegenres">
<a href="/prof/<%=@user.username%>"><h1 id="homeusername"><%= @user.username %></h1></a>
<ul>
	<%Genre.all().each do |n|%>
			<a href="/genre/<%=n.id%>"><li><%=n.name%></li></a>
	<%end%>
</ul>
<a href="/signout"><h2 id="homeusername">Sign Out</h2></a>
</div>
